{
  "name": "i18n-iso-util",
  "version": "1.2.3",
  "description": "A utility library for ISO-3166 and ISO 4217 codes",
  "homepage": "https://github.com/AdiRishi/i18n-iso-util",
  "bugs": {
    "url": "https://github.com/AdiRishi/i18n-iso-util/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdiRishi/i18n-iso-util.git"
  },
  "author": "Adishwar Rishi <adiswa123@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/gulp": "4.0.9",
    "@types/node": "16.11.11",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "del": "6.0.0",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "gulp": "4.0.2",
    "gulp-cli": "2.3.0",
    "husky": "7.0.4",
    "playwright": "1.17.1",
    "prettier": "2.5.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  },
  "scripts": {
    "clean": "gulp clean",
    "generate:iso3166": "ts-node scripts/extract-iso-3166-1.ts && yarn prettier",
    "prebuild": "yarn clean",
    "build": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && prettier --check .",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx && prettier --write .",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "prepublish": "yarn lint && yarn build"
  }
}
